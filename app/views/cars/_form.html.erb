<%= simple_form_for(car) do |f| %>
  <%= f.input :title %>
  <%= f.input :brand %>
  <%= f.input :model %>
  <%= f.input :year, as: :integer %>
  <%= f.input :price, as: :float %>
  <%= f.input :description %>
  <%= f.input :location %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit %>
<% end %>
